package sis.ui;

import javax.swing.text.DocumentFilter;

public class ChainableFilter extends DocumentFilter {
	private ChainableFilter next;
	public void setNext(ChainableFilter filter) {
		this.next = filter;
	}
	public ChainableFilter getNext() {
		return next;
	}
	public int getLimit() {
		return 0;
	} 
}

